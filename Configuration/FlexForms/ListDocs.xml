<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:pdflister/Resources/Private/Language/locallang_be.xlf:sheetGeneral</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.flex_startingpoint>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:pdflister/Resources/Private/Language/locallang_be.xlf:flex_startingpoint</label>
                            <config>
                                <type>group</type>
                                <internal_type>folder</internal_type>
                                <minitems>0</minitems>
                                <maxitems>1</maxitems>
                                <size>1</size>
                            </config>
                        </TCEforms>
                    </settings.flex_startingpoint>
                    <settings.flex_recursive>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:pdflister/Resources/Private/Language/locallang_be.xlf:flex_recursive</label>
                            <config>
                                <type>check</type>
                                <value>1</value>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </settings.flex_recursive>
                </el>
            </ROOT>
        </sDEF>
        <extraDataConfig>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:pdflister/Resources/Private/Language/locallang_be.xlf:extraDataConfig</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.flex_extradata>
                        <TCEforms>
                            <exclude>1</exclude>
                            <onChange>reload</onChange>
                            <label>LLL:EXT:pdflister/Resources/Private/Language/locallang_be.xlf:flex_extradata</label>
                            <config>
                                <type>check</type>
                                <value>1</value>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </settings.flex_extradata>
                    <settings.flex_datamessage>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.flex_extradata:REQ:true</displayCond>
                            <label>LLL:EXT:pdflister/Resources/Private/Language/locallang_be.xlf:flex_datamessage</label>
                            <config>
                                <type>text</type>
                            </config>
                        </TCEforms>
                    </settings.flex_datamessage>
                    <settings.flex_dateformat>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.flex_extradata:REQ:true</displayCond>
                            <label>LLL:EXT:pdflister/Resources/Private/Language/locallang_be.xlf:flex_dateformat</label>
                            <config>
                                <type>input</type>
                                <default>d/M/Y</default>
                                <size>4</size>
                            </config>
                        </TCEforms>
                    </settings.flex_dateformat>
                    <settings.flex_timeformat>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.flex_extradata:REQ:true</displayCond>
                            <label>LLL:EXT:pdflister/Resources/Private/Language/locallang_be.xlf:flex_timeformat</label>
                            <config>
                                <type>input</type>
                                <default>H:i</default>
                                <size>4</size>
                            </config>
                        </TCEforms>
                    </settings.flex_timeformat>
                    <settings.flex_adduserdata>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.flex_extradata:REQ:true</displayCond>
                            <label>LLL:EXT:pdflister/Resources/Private/Language/locallang_be.xlf:flex_adduserdata</label>
                            <config>
                                <type>check</type>
                                <value>1</value>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </settings.flex_adduserdata>
                </el>
            </ROOT>
        </extraDataConfig>
        <appearanceConfig>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:pdflister/Resources/Private/Language/locallang_be.xlf:appearance</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.flex_font>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:pdflister/Resources/Private/Language/locallang_be.xlf:flex_font</label>
                            <config>
                                <type>input</type>
                                <default>Helvetica</default>
                                <size>10</size>
                                <eval>alpha</eval>
                            </config>
                        </TCEforms>
                    </settings.flex_font>
                    <settings.flex_font_size>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:pdflister/Resources/Private/Language/locallang_be.xlf:flex_font_size</label>
                            <config>
                                <type>input</type>
                                <default>10</default>
                                <size>2</size>
                                <eval>int</eval>
                            </config>
                        </TCEforms>
                    </settings.flex_font_size>
                    <settings.flex_posxy>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:pdflister/Resources/Private/Language/locallang_be.xlf:flex_posxy</label>
                            <config>
                                <type>input</type>
                                <default>10,0</default>
                                <size>4</size>
                            </config>
                        </TCEforms>
                    </settings.flex_posxy>
                    <settings.flex_posxy_sub>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:pdflister/Resources/Private/Language/locallang_be.xlf:flex_posxy_sub</label>
                            <config>
                                <type>input</type>
                                <default>10,2</default>
                                <size>4</size>
                            </config>
                        </TCEforms>
                    </settings.flex_posxy_sub>
                    <settings.flex_cellsize>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:pdflister/Resources/Private/Language/locallang_be.xlf:flex_cellsize</label>
                            <config>
                                <type>input</type>
                                <default>40,10</default>
                                <size>4</size>
                            </config>
                        </TCEforms>
                    </settings.flex_cellsize>
                </el>
            </ROOT>
        </appearanceConfig>
    </sheets>
</T3DataStructure>
